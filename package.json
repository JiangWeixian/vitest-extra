{
  "name": "vitest-extra",
  "version": "0.0.4",
  "license": "MIT",
  "packageManager": "pnpm@10.15.0",
  "homepage": "https://github.com/JiangWeixian/vitest-extra#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JiangWeixian/vitest-extra.git"
  },
  "bugs": {
    "url": "https://github.com/JiangWeixian/vitest-extra/issues",
    "email": "jiangweixian1994@gmail.com"
  },
  "author": "JW",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./dist/index.cjs",
      "types": "./types.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "types.d.ts",
  "files": [
    "dist",
    "types.d.ts"
  ],
  "scripts": {
    "build": "rimraf dist && rollup -c --silent",
    "dev": "rollup -c -w --silent",
    "test": "vitest",
    "prepare": "husky install",
    "pretest": "pnpm run build",
    "ci:version": "pnpm changeset version",
    "ci:publish": "pnpm run build && pnpm changeset publish",
    "ci:snapshot": "pnpm changeset version --snapshot beta",
    "ci:prerelease": "pnpm run build && pnpm changeset publish --tag beta",
    "lint:fix": "eslint . --fix"
  },
  "lint-staged": {
    "**/**/*.{js,ts,tsx,vue,json}": [
      "eslint --fix"
    ]
  },
  "peerDependencies": {
    "vitest": ">=0.7.7"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "fs-extra": "10.0.1",
    "globby": "13.1.1"
  },
  "devDependencies": {
    "@aiou/eslint-config": "^0.5.1",
    "@changesets/cli": "^2.21.1",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/debug": "^4.1.8",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.22",
    "c8": "^7.11.0",
    "cz-emoji": "^1.3.1",
    "eslint": "^8.11.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "lodash-es": "^4.17.21",
    "picocolors": "^1.0.0",
    "prettier": "2.4.1",
    "rimraf": "3.0.2",
    "rollup": "2.70.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-condition-exports": "^1.3.1",
    "rollup-plugin-node-externals": "^4.0.0",
    "rollup-plugin-size": "^0.2.2",
    "rollup-plugin-typescript2": "0.30.0",
    "tslib": "2.3.1",
    "typescript": "4.4.3",
    "vitest": "^0.7.7"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-emoji"
    }
  }
}
